{
  "name": "incremental-capture-cli",
  "version": "0.0.1",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src && prettier --check .",
    "format": "eslint ./src --fix && prettier --write .",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "build": "npm run clean && npm run tsc && fix-tsc-es-imports -y -V && chmod +x dist/index.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "fix-tsc-es-imports": "^0.1.6",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "bin": "./dist/index.js",
  "dependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/progress": "^2.0.7",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.15",
    "progress": "^2.0.3"
  }
}

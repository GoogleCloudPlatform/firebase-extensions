{
  "name": "firestore-genai-chatbot",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "eslint \"src/**/*\" --fix",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md",
    "test": "vitest run",
    "test:watch": "vitest",
    "publish-from-main": "firebase ext:dev:upload googlecloud/firestore-genai-chatbot --repo=https://github.com/googlecloudplatform/firebase-extensions --root=firestore-genai-chatbot --ref=main --project pub-ext-gcloud"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@genkit-ai/googleai": "^0.9.2",
    "@genkit-ai/vertexai": "^0.9.2",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^4.9.0",
    "genkit": "^0.9.2",
    "google-auth-library": "^9.0.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.15.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "firebase-functions-test": "^3.0.0",
    "google-gax": "^4.3.2",
    "node-gyp": "^10.1.0",
    "ts-node": "^10.9.2",
    "vitest": "^2.1.5"
  },
  "files": [
    "lib"
  ],
  "private": true
}

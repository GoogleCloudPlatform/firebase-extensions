  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/vision": "^4.2.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.2.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "dotenv": "^16.0.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^3.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5"
  },
  "private": true
}

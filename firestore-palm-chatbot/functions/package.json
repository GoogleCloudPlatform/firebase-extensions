{
  "name": "firestore-palm-chatbot",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "eslint \"src/**/*\" --fix",
    "prebuild": "cd ../../common/ext-gen-ai && npm run build && cd ../../firestore-palm-chatbot/functions && npm pack ../../common/* && npm install --save *.tgz",
    "build": "npm run prebuild && tsc",
    "build:watch": "npm run prebuild && tsc --watch",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md",
    "test": "jest --runInBand"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-ai/generativelanguage": "^1.0.0",
    "@google-cloud/aiplatform": "^3.0.0",
    "ext-gen-ai": "file:ext-gen-ai-1.0.0.tgz",
    "firebase-admin": "^11.6.0",
    "firebase-functions": "^4.3.0",
    "google-auth-library": "^9.0.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.15.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "firebase-functions-test": "^3.0.0",
    "google-gax": "^3.6.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.4.0"
  },
  "files": [
    "lib"
  ],
  "private": true
}
{
  "name": "bigquery-firestore-export",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run compile",
    "build:watch": "npm run clean && tsc --watch",
    "clean": "rimraf lib",
    "compile": "tsc",
    "local:emulator": "cd ../../_emulator && firebase emulators:start -P demo-test",
    "test": "cd ../../_emulator && firebase emulators:exec jest -P demo-test",
    "test:local": "concurrently \"npm run local:emulator\" \"jest\"",
    "test:watch": "concurrently \"npm run local:emulator\" \"jest --watch\"",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/bigquery": "^6.1.0",
    "@google-cloud/bigquery-data-transfer": "^3.2.1",
    "@google-cloud/firestore": "^6.5.0",
    "@types/express-serve-static-core": "4.17.30",
    "@types/jest": "^29.5.0",
    "@types/node": "14.18.34",
    "@types/nodemailer": "^6.2.1",
    "firebase-admin": "^11.4.1",
    "firebase-functions": "^4.3.1",
    "handlebars": "^4.5.3",
    "install": "^0.13.0",
    "lodash.mapvalues": "^4.6.0",
    "nodemailer": "^6.9.16",
    "npm": "^9.6.2",
    "rimraf": "^2.6.3",
    "smtp-server": "^3.13.6",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@google-cloud/pubsub": "^3.5.2",
    "@types/lodash.mapvalues": "^4.6.7",
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.3",
    "firebase-functions-test": "^0.2.3",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "mocked-env": "^1.3.2",
    "ts-jest": "^24.1.0"
  },
  "files": [
    "lib"
  ],
  "private": true
}

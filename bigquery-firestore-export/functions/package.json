{
  "name": "bigquery-firestore-export",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run compile",
    "build:watch": "npm run clean && tsc --watch",
    "clean": "rimraf lib",
    "compile": "tsc",
    "local:emulator": "cd ../../_emulator && firebase emulators:start -P demo-test",
    "test": "jest --runInBand",
    "test:local": "concurrently \"npm run local:emulator\" \"jest\"",
    "test:watch": "concurrently \"npm run local:emulator\" \"jest --watch\"",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/bigquery": "^6.1.0",
    "@google-cloud/bigquery-data-transfer": "^5.0.1",
    "@google-cloud/firestore": "^7.11.2",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.3.2",
    "lodash.mapvalues": "^4.6.0",
    "rimraf": "^2.6.3",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@google-cloud/pubsub": "^4.10.0",
    "@types/jest": "^29.5.14",
    "@types/lodash.mapvalues": "^4.6.7",
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.3",
    "firebase-functions-test": "^3.4.1",
    "jest": "^29.7.0",
    "mocked-env": "^1.3.2",
    "ts-jest": "^29.3.0"
  },
  "files": [
    "lib"
  ],
  "private": true
}

{
  "name": "bigquery-firestore-export",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run compile",
    "build:watch": "npm run clean && tsc --watch",
    "clean": "rimraf lib",
    "compile": "tsc",
    "local:emulator": "cd ../../_emulator && firebase emulators:start -P demo-test",
    "test": "cd ../../_emulator && firebase emulators:exec jest -P demo-test",
    "test:local": "concurrently \"npm run local:emulator\" \"jest\"",
    "test:watch": "concurrently \"npm run local:emulator\" \"jest --watch\"",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/bigquery": "^7.6.0",
    "@google-cloud/bigquery-data-transfer": "^4.2.1",
    "@google-cloud/firestore": "^7.6.0",
    "@types/express-serve-static-core": "4.17.30",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.12.7",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^4.9.0",
    "lodash.mapvalues": "^4.6.0",
    "nodemailer": "^6.3.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/lodash.mapvalues": "^4.6.7",
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.3",
    "firebase-functions-test": "^3.2.0",
    "jest": "^29.7.0",
    "mocked-env": "^1.3.5",
    "ts-jest": "^29.1.2"
  },
  "files": [
    "lib"
  ],
  "private": true
}

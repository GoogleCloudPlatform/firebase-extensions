    "@google-cloud/vertexai": "^0.5.0",
    "@google/generative-ai": "^0.24.0",
    "@types/jest": "^29.5.12",
    "@types/mustache": "^4.2.2",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^4.9.0",
    "genkit": "^1.24.0",
    "google-auth-library": "^9.0.0",
    "mustache": "^4.2.0",
    "sharp": "^0.34.5",
    "typescript": "^4.9.0",

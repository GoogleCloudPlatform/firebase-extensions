    "eslint": "^9.39.2",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "firebase-functions-test": "^3.1.0",
    "jest": "^29.7.0",
    "node-gyp": "^12.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "wait-for-expect": "^3.0.2"
